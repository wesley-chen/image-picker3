{
  "name": "Image Picker v3",
  "description": "Choose (filter) images and save images from web page to local disk.",
  "version": "3.0.1",
  "manifest_version": 2,
  "icons": {
    "32": "icons/img-picker_32.png",
    "48": "icons/img-picker_32.png",
    "128": "icons/img-picker_32.png"
  },
  "permissions": ["activeTab", "tabs", "storage", "downloads", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
  "web_accessible_resources": ["fonts/*"],
  "browser_action": {
    "default_title": "Image Picker v3"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}
